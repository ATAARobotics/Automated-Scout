{
  "name": "automated-scout",
  "version": "0.22.0",
  "description": "",
  "main": "",
  "license": "ISC",
  "scripts": {
    "build": "esbuild --bundle --outdir=client/dist client/source/main.tsx",
    "start": "esbuild --bundle --watch --sourcemap --outdir=client/dist client/source/main.tsx",
    "lint": "tsc --noEmit --pretty && eslint client/source/**/*.{ts,tsx}",
    "fix": "prettier -w client/source/**/*.{ts,tsx} && eslint --fix client/source/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ATAARobotics/Automated-Scout20"
  },
  "author": "ATAA",
  "dependencies": {
    "esbuild": "^0.14.36",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "rsuite": "^5.8.1",
    "rsuite-table": "^5.4.1"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "parserOptions": {
          "project": "tsconfig.json",
          "ecmaVersion": 2020,
          "sourceType": "module"
        }
      }
    ],
    "settings": {
      "ecmascript": 7
    },
    "env": {
      "browser": true
    },
    "plugins": [
      "@typescript-eslint",
      "eslint-plugin-jsdoc",
      "react"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "quotes": [
        "warn",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "comma-style": [
        "warn",
        "last"
      ],
      "brace-style": [
        "warn",
        "1tbs"
      ],
      "func-call-spacing": [
        "warn",
        "never"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "comma-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "spaced-comment": [
        "warn",
        "always",
        {
          "markers": [
            "/"
          ]
        }
      ],
      "semi-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "multiline-comment-style": [
        "warn",
        "separate-lines"
      ],
      "no-label-var": "warn",
      "no-useless-rename": "warn",
      "new-cap": "warn",
      "camelcase": [
        "warn",
        {
          "ignoreImports": true
        }
      ],
      "no-unused-vars": "off",
      "react/jsx-uses-vars": "warn",
      "object-property-newline": "off",
      "eqeqeq": [
        "error",
        "always"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-var": "error",
      "strict": [
        "error",
        "global"
      ],
      "no-confusing-arrow": "error",
      "no-shadow": [
        "error",
        {
          "builtinGlobals": true,
          "hoist": "all",
          "allow": []
        }
      ],
      "@typescript-eslint/strict-boolean-expressions": "error",
      "@typescript-eslint/no-empty-function": "off",
      "jsdoc/require-param-type": "off",
      "jsdoc/require-returns-type": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jsdoc/recommended"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-plugin-jsdoc": "^39.2.4",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "18.0.5",
    "react-is": "17.0.2",
    "semver": "7.3.7",
    "eslint-visitor-keys": "3.3.0",
    "eslint-scope": "7.1.1",
    "glob-parent": "6.0.2",
    "estraverse": "5.3.0",
    "doctrine": "3.0.0"
  }
}
